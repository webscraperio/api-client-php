{
    "name": "webscraperio/api-client-php",
    "description": "API client for webscraper.io",
    "type": "library",
    "keywords": ["web scraper", "webscraperio"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WebScraper\\ApiClient\\": "src/ApiClient"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
